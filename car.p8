function _update60()
	if (btn(➡️)) p.x+=1
	if (btn(⬅️)) p.x-=1
	if (btn(⬆️)) p.y-=1
	if (btn(⬇️)) p.y+=1

end

function _draw()
	cls(15)
	spr(1,p.x,p.y)
	for e in all(enemies) do 
	spr(7,e.x,e.y)
 end
end
